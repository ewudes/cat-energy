  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "^2.0.7",
    "autoprefixer": "10.4.x",
    "browser-sync": "^3.0.2",
    "del": "^5.1.0",
    "gulp": "^3.9.1",
